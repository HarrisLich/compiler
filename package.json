{
  "name": "personal",
  "version": "0.0.1",
  "description": "Personal",
  "keywords": [
    "personal"
  ],
  "license": "ISC",
  "author": "Harris Lichstein",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc -p tsconfig.json",
    "dev": "npx tsx src/index.ts",
    "debug": "DEBUG=app:* yarn dev",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "testOld": "jest --passWithNoTests",
    "test": "vitest",
    "test:ci": "vitest run --coverage --passWithNoTests",
    "prepare": "husky",
    "typecheck": "tsc --noEmit --pretty",
    "prepack": "yarn lint && yarn typecheck && yarn build"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx, js}": [
      "eslint --max-warnings=0 --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier -w"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "chore"
        ]
      ],
      "subject-case": [
        2,
        "always",
        "sentence-case"
      ]
    }
  }
}
